r\n    }\r\n    let [newShowTip, setNewShowTip] = useState(showTip ? showTip : custom)\r\n    let [val, setVal] = useState(value)\r\n    let [textNull, setTextNull] = useState(false)\r\n    let showBorder = newShowTip && newShowTip !== true && newShowTip.border === true\r\n    let [tip, setTip] = useState(inputTextNullTip)\r\n    const classes = classNames(\"eda-input-inner\", className, {\r\n        'eda-input-disabled': disabled,\r\n        'eda-input-group-addon-before': addonAfter,\r\n        'eda-input-group-addon-after': addonBefore,\r\n        'eda-input-null': ((newShowTip === true) && textNull) || (showBorder && textNull)\r\n    });\r\n    const inputGroup = classNames(\"eda-input-group\", \"\", {\r\n        'eda-input-disabled': disabled,\r\n    });\r\n    const tips = (newShowTip && textNull)\r\n    useEffect(() => {\r\n        setVal(value)\r\n    }, [value])\r\n    useEffect(() => {\r\n        setTextNull(inputNull)\r\n    }, [inputNull])\r\n    const changHandle = (event: React.ChangeEvent<HTMLInputElement>) => {\r\n        let value: string = event.target.value\r\n        !disabled && setVal(value)\r\n        if (onChangeInput && !disabled) {\r\n            onChangeInput(value)\r\n        }\r\n    }\r\n    const focusHandle = (event: React.FocusEvent<HTMLInputElement>) => {\r\n        if (onFocus) {\r\n            onFocus(event)\r\n        }\r\n    }\r\n    const blurHandle = (event: React.FocusEvent<HTMLInputElement>) => {\r\n        if (val === \"\") {\r\n            // setTip(TEXT_NULL)\r\n            setTextNull(true)\r\n        } else {\r\n            setTextNull(false)\r\n        }\r\n        let flag = false\r\n        if (min && min > val.length) {\r\n            setTip(MIN_TIP + min)\r\n            flag = true\r\n        } else if (max && max < val.length) {\r\n            setTip(MAX_TIP + max)\r\n            flag = true\r\n        } else if (textType === \"number\" && !isNumber(val)) {\r\n            setTip(NUMBER_TIP)\r\n            flag = true\r\n        } else if (textType === \"email\" && !isEmail(val)) {\r\n            setTip(EMAIL_TIP)\r\n            flag = true\r\n        }\r\n\r\n        if (flag) {\r\n            setTextNull(true)\r\n        }\r\n        if (onBlur) {\r\n            onBlur(event)\r\n        }\r\n\r\n    }\r\n    return <div className=\"eda-input\">\r\n\r\n        <div className={inputGroup}>\r\n            {addonBefore ? <span className=\"eda-input-group-addon eda-input-group-addon-before\">{addonBefore}</span> : null}\r\n            <input value={val} onChange={changHandle}\r\n                className={classes} type={type ? type : \"text\"}\r\n                disabled={disabled}\r\n                onBlur={blurHandle}\r\n                onFocus={focusHandle}\r\n                {...restProps} />\r\n            {addonAfter ? <span className=\"eda-input-group-addon eda-input-group-addon-after\">{addonAfter}</span> : null}\r\n\r\n        </div>\r\n        {tips ? <div className=\"inputTextNullTip\">{tip}</div> : null}\r\n    </div>\r\n}\r\nInput.displayName = \"Input\";\r\nexport default Input\r\n","D:\\Users\\Administrator\\Desktop\\eda-personal-center\\src\\components\\eda-design\\icon\\Icon.tsx",[],"D:\\Users\\Administrator\\Desktop\\eda-personal-center\\src\\components\\eda-design\\select\\select.tsx",[],"D:\\Users\\Administrator\\Desktop\\eda-personal-center\\src\\components\\eda-design\\select\\option.tsx",["304"],"import React, { FC, useContext, HTMLAttributes, useEffect } from \"react\";\r\nimport { classNames } from \"../utils\";\r\nimport \"./select.less\";\r\nimport { SelectContext } from './select'\r\nexport interface BaseOptionProps {\r\n    value: string;\r\n}\r\ntype NativeOptionProps = BaseOptionProps & HTMLAttributes<HTMLElement>;\r\n//Partial 设置为可选属性\r\nexport type OptionProps = Partial<NativeOptionProps>;\r\nexport const Option: FC<OptionProps> = (props) => {\r\n    const { className, value = \"\", children, ...restProps } = props;\r\n\r\n    const context = useContext(SelectContext);\r\n    const classes = classNames(\"eda-option eda-select-dropdown__item\", className, {\r\n        \"eda-select-option\": context.selectValue === value\r\n    });\r\n\r\n    const clickHandle = () => {\r\n        if (context.onChange) {\r\n            context.onChange(value, children)\r\n        }\r\n    }\r\n    useEffect(() => {\r\n        if (context.selectValue === value && context.onChange) {\r\n            context.onChange(value, children, \"first\")\r\n        }\r\n    }, [context.selectValue])\r\n\r\n    return (\r\n        <li className={classes}  {...restProps} onMouseDown={() => {\r\n            clickHandle()\r\n        }}>\r\n            {children}\r\n        </li>\r\n    );\r\n};\r\nexport default Option;\r\n","D:\\Users\\Administrator\\Desktop\\eda-personal-center\\src\\components\\eda-design\\avatar\\avatar.tsx",[],"D:\\Users\\Administrator\\Desktop\\eda-personal-center\\src\\components\\eda-design\\textarea\\textarea.tsx",[],["305","306"],"D:\\Users\\Administrator\\Desktop\\eda-personal-center\\src\\components\\eda-design\\switch\\switch.tsx",[],"D:\\Users\\Administrator\\Desktop\\eda-personal-center\\src\\components\\eda-design\\alert\\alert.tsx",[],"D:\\Users\\Administrator\\Desktop\\eda-personal-center\\src\\components\\eda-design\\badge\\badge.tsx",[],"D:\\Users\\Administrator\\Desktop\\eda-personal-center\\src\\components\\eda-design\\mask\\mask.tsx",[],"D:\\Users\\Administrator\\Desktop\\eda-personal-center\\src\\components\\eda-design\\modal\\modal.tsx",[],"D:\\Users\\Administrator\\Desktop\\eda-personal-center\\src\\components\\eda-design\\spin\\spin.tsx",[],"D:\\Users\\Administrator\\Desktop\\eda-personal-center\\src\\components\\eda-design\\message\\message.tsx",[],["307","308"],"D:\\Users\\Administrator\\Desktop\\eda-personal-center\\src\\components\\eda-design\\menu\\subMenu.tsx",[],"D:\\Users\\Administrator\\Desktop\\eda-personal-center\\src\\components\\eda-design\\tag\\tag.tsx",[],"D:\\Users\\Administrator\\De